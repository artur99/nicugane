<!DOCTYPE html>
<html lang="ro">
<head>
    {% include 'part/head.twig' %}
</head>
<body class="landing">
    {% include 'part/header.twig' %}

    <div class="header">
        <div class="headerin">
            <h1>{{title}}</h1>
            <span class="bcnav">
                <a href="{{l('/')}}">CNNG</a>
                <i class="material-icons">&#xE315;</i>
                <a href="#">{{title}}</a>
            </span>
        </div>
    </div>
    <div class="sec proflist">
        <div class="cin">

            <h2 class="subtitle">Profesorii</h2>
            <div class="lines"></div>
            {% for item in profs %}
                <a href="{{l('/profesori/'~item.id~'-'~(item.name|slugify))}}" class="profitem light-effect-p z-depth-1">
                    <div class="prof-header light-effect" {{item.image ? ('style="'~gen_bgcss(item.image)~'"')|raw}}></div>
                    <div class="prof-cont">
                        <div>
                            {% if item.data.statut is defined %}
                            <h5>{{item.data.statut}}</h5>
                            {% endif %}
                            <h3>{{item.name}}</h3>
                            {% if item.data.spec is defined %}
                            <h4>{{item.data.spec|join(' & ')}}</h4>
                            {% endif %}
                        </div>
                    </div>
                </a>
            {% endfor %}
        </div>
    </div>

    {% include 'part/footer.twig' %}

    {% include 'part/scripts.twig' %}
</body>
</html>
